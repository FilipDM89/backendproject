<% include partials/header.ejs %>
<div class="container">
    <div>
        <h2><%= historyarticle.title %></h2>
    </div>
    <div>
        <img src="<%= historyarticle.image %>">
    </div>
    <div>
        <strong><%= historyarticle.created.toDateString() %></strong>
    </div>
    <div>
        <%= historyarticle.content %>
    </div>
    <form action="/history/<%= historyarticle._id %>?_method=DELETE" method="POST">
        <button>Delete</button>
    </form>
</div>

<% include partials/footer.ejs %>